{
  "name": "oanhnn/laravel-logzio",
  "type": "library",
  "description": "Integrate Logz.io into Laravel 5.6+",
  "keywords": [
    "laravel",
    "package",
    "log",
    "logging",
    "logzio",
    "logz.io"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oanhnn/laravel-logzio",
  "support": {
    "issues": "https://github.com/oanhnn/laravel-logzio/issues",
    "source": "https://github.com/oanhnn/laravel-logzio"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laravel\\Logzio\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "monolog/monolog": "~1.12",
    "illuminate/contracts": "^5.6",
    "illuminate/support": "^5.6"
  },
  "require-dev": {
    "mockery/mockery": "^0.9",
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "~6.1|~7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html storage/coverage",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Logzio\\LogzioServiceProvider"
      ]
    }
  }
}
